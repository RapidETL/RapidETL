{
    "name": "sample_transform",
    "source": [
        {
            "type": "csv",
            "config": {
                "path": "data/input.csv",
                "delimiter": ",",
                "has_header": true
            }
        }
    ],
    "sink": [
        {
            "type": "csv",
            "config": {
                "path": "data/output/transformed.csv",
                "delimiter": ",",
                "include_header": true
            }
        }
    ]
}